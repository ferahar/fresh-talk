{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ESNext",
    "module": "AMD",
    "outFile": "./dist/bundle.js",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
  },
  "files":[
    "./src/index.ts",
  ],
}